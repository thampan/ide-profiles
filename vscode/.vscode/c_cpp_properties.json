{
    "configurations": [
        {
            "name": "MicrosoftIntellisenseConfig",
            "includePath": [
                "/usr/include/**",
            ],
            "compilerPath": "/user/bin/g++",
            "cppStandard": "c++20",
            "cStandard": "c11",
            "intelliSenseMode": "linux-gcc-x86",
            "configurationProvider": "ms-vscode.cmake-tools",
            "compilerArgs": [
                "--std=c++2a"
            ]
        },
        {
        	"name": "ClangdConfig",
        	"compileCommands": "${workspaceFolder}/../build/sim.build/compile_commands.json",
        	"cppStandard": "c++20",
        	"intelliSenseMode": "linux-gcc-x86",
        	"includePath": ["${workspaceFolder}/../build/sim.build/Protobuf/ProtoOutFiles/Protobuf/**"],
        	"browse": {
        		"path": ["${workspaceFolder}/../build/sim.build/Protobuf/ProtoOutFiles/Protobuf/**"]
        	}
        }
    ],
    "version": 4
}
